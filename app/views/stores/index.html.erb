<h1>Stores</h1>

<table width="100%">
  <tr>
    <th>Name</th>
    <th>Tel</th>
    <th>Fax</th>
    <th>Email</th>
    <th>Contact</th>
    <th></th>
    <th></th>
  </tr>

<% @stores.each do |store| %>
  <tr>
    <td><%= store.name %></td>
    <td><%= store.tel %></td>
    <td><%= store.fax %></td>
    <td><%= store.email %></td>
    <td><%= store.contact %></td>
    <td><%= link_to 'Show', store %></td>
    <td><%= link_to 'Edit', edit_store_path(store) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Store', new_store_path %>
